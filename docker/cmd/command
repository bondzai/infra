# sonarqube
docker run -d --name sonarqube -p 9000:9000 -p 9092:9092 sonarqube

# hivemq
docker run -p 8080:8080 -p 1883:1883 hivemq/hivemq4

# ngrok
docker pull wernight/ngrok
docker run -d --name ngrok -p 4040:4040 -p 51820:51820/udp wernight/ngrok ngrok udp --authtoken=your_ngrok_authtoken --region=us --log=stdout 51820
curl -s http://localhost:4040/api/tunnels | grep -oP "public_url\":\"udp://\K[^\"]+"
